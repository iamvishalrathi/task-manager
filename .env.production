# Production Environment for Demo Deployment
# Add this as environment variables in Vercel/Netlify

# Enable MSW in production for demo purposes
VITE_ENABLE_MSW=true

# App Configuration
VITE_APP_NAME="Task Manager Pro"
VITE_APP_VERSION="2.0.0"
VITE_APP_DESCRIPTION="Production-ready task management application"

# Demo Mode Configuration
VITE_DEMO_MODE=true
VITE_DEMO_USERNAME=demo@example.com
VITE_DEMO_PASSWORD=password

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_PERFORMANCE_MONITORING=false
VITE_ENABLE_DARK_MODE=true

# MSW Configuration
VITE_MSW_LOGGING=false
